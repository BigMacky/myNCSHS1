<p id="notice"><%= notice %></p>

<h1>Listing Teacher Subject Sections</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Subject title</th>
      <th>Section name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_subject_sections.each do |teacher_subject_section| %>
      <tr>
        <td><%= teacher_subject_section.teacher_id %></td>
        <td><%= teacher_subject_section.subject_title %></td>
        <td><%= teacher_subject_section.section_name %></td>
        <td><%= link_to 'Show', teacher_subject_section %></td>
        <td><%= link_to 'Edit', edit_teacher_subject_section_path(teacher_subject_section) %></td>
        <td><%= link_to 'Destroy', teacher_subject_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher subject section', new_teacher_subject_section_path %>
