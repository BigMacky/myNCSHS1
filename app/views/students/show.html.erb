<body id="home">
	<div id="wrapper">
		<!-- insert the name of student here !-->
		<span id="name">Logged in as: <%= @student.first_name%> <%=@student.middle_name%> <%=@student.last_name%>, <%=@student.year_level%>nd Year</span>
		<div id="wrapper">
			<div id="menu">
				<img src="/assets/img/ncshs.gif" id="logo"/>
			</div>
			<div id="form2">
					<a href="/students/home" class="menuitem">HOME</a>
					<a href="grades.html" class="menuitem">GRADES</a>
					<a href="schedule.html" class="menuitem">SCHEDULE</a>
					<a href="curriculum.html" class="menuitem">CURRICULUM</a>
					<a href="/students/login"><img src="/assets/img/out.png" id="logout"/></a>
					<a href="/students/<%= @student.id%>"><img src="/assets/img/profile.png" id="profile"/></a>
			</div>

			<div id="content">
				<p id="notice"><%= notice %></p>

				<p>
					<strong>Student LRN:</strong>
					<%= @student.student_lrn %>
				</p>

				<p>
					<strong>First Name:</strong>
					<%= @student.first_name %>
				</p>

				<p>
					<strong>Middle Name:</strong>
					<%= @student.middle_name %>
				</p>

				<p>
					<strong>Last Name:</strong>
					<%= @student.last_name %>
				</p>

				<p>
					<strong>Age:</strong>
					<%= @student.age %>
				</p>

				<p>
					<strong>Birthdate:</strong>
					<%= @student.birthdate %>
				</p>

				<p>
					<strong>Religion:</strong>
					<%= @student.religion %>
				</p>

				<p>
					<strong>Contact Number:</strong>
					<%= @student.contact_no %>
				</p>

				<p>
					<strong>Year Level:</strong>
					<%= @student.year_level %>
				</p>

				<!--<%= link_to 'Edit', edit_student_path(@student) %> |
				<%= link_to 'Back', students_path %> !-->
			</div>
		</div>
	</div>
</body>