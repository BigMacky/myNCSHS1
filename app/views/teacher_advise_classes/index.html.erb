<p id="notice"><%= notice %></p>

<h1>Listing Teacher Advise Classes</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Section name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_advise_classes.each do |teacher_advise_class| %>
      <tr>
        <td><%= teacher_advise_class.teacher_id %></td>
        <td><%= teacher_advise_class.section_name %></td>
        <td><%= link_to 'Show', teacher_advise_class %></td>
        <td><%= link_to 'Edit', edit_teacher_advise_class_path(teacher_advise_class) %></td>
        <td><%= link_to 'Destroy', teacher_advise_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher advise class', new_teacher_advise_class_path %>
